            android:layout_height="fill_parent"></TableRow>

        <TableRow
            android:layout_width="fill_parent"
            android:layout_height="fill_parent">

        </TableRow>

    </TableLayout>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="Large Text"
        android:id="@+id/caloriesCount"
        android:paddingTop="35dp"
        android:layout_alignTop="@+id/colourCode"
        android:layout_alignRight="@+id/minimumCalories"
        android:layout_alignEnd="@+id/minimumCalories" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Add To Diary"
        android:id="@+id/button6"
        android:layout_alignTop="@+id/colourCode"
        android:layout_toRightOf="@+id/colourCode"
        android:layout_marginLeft="20dp"
        android:layout_alignBottom="@+id/colourCode"
        android:onClick="addToDiary" />

</RelativeLayout>