<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent" android:layout_height="match_parent">

    <TextView
        android:layout_width="200dp"
        android:layout_height="100dp"
        android:id="@+id/colourCode"
        android:layout_marginTop="107dp"
        android:background="#ff09ff00"
        android:layout_marginLeft="42dp"
        android:layout_marginStart="42dp"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="@string/minimumCaloriesText"
        android:id="@+id/minCaloriesText"
        android:layout_marginTop="30dp"
        android:layout_below="@+id/colourCode"
        android:layout_alignLeft="@+id/colourCode"
        android:layout_alignStart="@+id/colourCode"
        android:singleLine="false" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="@string/something"
        android:id="@+id/minimumCalories"
        android:layout_alignTop="@+id/minCaloriesText"
        android:layout_toRightOf="@+id/minCaloriesText"
        android:layout_toEndOf="@+id/minCaloriesText"
        android:layout_marginLeft="10dp"
        android:singleLine="false" />

    <TableLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:id="@+id/tableLayout"
        android:paddingTop="30dp"
        android:layout_below="@+id/minCaloriesText"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginTop="62dp"
        android:layout_marginRight="20dp"
        android:layout_marginLeft="20dp">

        <TableRow
            android:layout_width="fill_parent"
            android:layout_height="fill_parent" >

            <TextView
                android:text="         "
                android:gravity="center"
                android:layout_column="1"
                android:id="@+id/empty"
                android:inputType="none"
                android:paddingLeft="15dp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" />

            <TextView
                android:text="Goal"
                android:gravity="center"
                android:layout_column="2"
                android:id="@+id/goal"
                android:inputType="none"
                android:paddingLeft="20dp"
                android:paddingBottom="10dp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" />

            <TextView
                android:text="Consumed"
                android:gravity="center"
                android:layout_column="3"
                android:id="@+id/consumed"
                android:inputType="none"
                android:paddingLeft="20dp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" />

            <TextView
                android:text="Remaining"
                android:gravity="center"
                android:layout_column="4"
                android:id="@+id/remaining"
                android:inputType="none"
                android:paddingLeft="20dp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" />
        </TableRow>

        <TableRow
            android:layout_width="fill_parent"
            android:layout_height="fill_parent" >

            <TextView
                android:text="Protein"
                android:gravity="center"
                android:layout_column="1"
                android:id="@+id/prots"
                android:inputType="none"
                android:paddingLeft="15dp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" />

            <TextView
                android:gravity="center"
                android:layout_column="2"
                android:id="@+id/protsGoal"
                android:paddingLeft="20dp"
                android:paddingBottom="10dp"
                android:maxLines="10"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" />

            <TextView
                android:text="numberC"
                android:gravity="center"
                android:layout_column="3"
                android:id="@+id/protsCons"
                android:inputType="none"
                android:paddingLeft="20dp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" />

            <TextView
                android:text="numberL"
                android:gravity="center"
                android:layout_column="4"
                android:id="@+id/protsRem"
                android:inputType="none"
                android:paddingLeft="20dp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" />
        </TableRow>

        <TableRow
            android:layout_width="fill_parent"
            android:layout_height="fill_parent" >

            <TextView
                android:text="Carbohydrate"
                android:gravity="center"
                android:layout_column="1"
                android:id="@+id/carbs"
                android:inputType="none"
                android:paddingLeft="15dp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" />

            <TextView
                android:text="NumberG"
                android:gravity="center"
                android:layout_column="2"
                android:id="@+id/carbsGoal"
                android:inputType="none"
                android:paddingLeft="20dp"
                android:paddingBottom="10dp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" />

            <TextView
                android:text="numberC"
                android:gravity="center"
                android:layout_column="3"
                android:id="@+id/carbsCons"
                android:inputType="none"
                android:paddingLeft="20dp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" />

            <TextView
                android:text="numberL"
                android:gravity="center"
                android:layout_column="4"
                android:id="@+id/carbsRem"
                android:inputType="none"
                android:paddingLeft="20dp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" />
        </TableRow>

        <TableRow
            android:layout_width="fill_parent"
            android:layout_height="fill_parent" >

            <TextView
                android:text="Fat"
                android:gravity="center"
                android:layout_column="1"
                android:id="@+id/fat"
                android:inputType="none"
                android:paddingLeft="15dp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" />

            <TextView
                android:text="NumberG"
                android:gravity="center"
                android:layout_column="2"
                android:id="@+id/fatGoal"
                android:inputType="none"
                android:paddingLeft="20dp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" />

            <TextView
                android:text="numberC"
                android:gravity="center"
                android:layout_column="3"
                android:id="@+id/fatCons"
                android:inputType="none"
                android:paddingLeft="20dp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" />

            <TextView
                android:text="numberL"
                android:gravity="center"
                android:layout_column="4"
                android:id="@+id/fatRem"
                android:inputType="none"
                android:paddingLeft="20dp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" />
        </TableRow>

        <TableRow
            android:layout_width="fill_parent"
            android:layout_height="fill_parent"></TableRow>

        <TableRow
            android:layout_width="fill_parent"
            android:layout_height="fill_parent"></TableRow>

        <TableRow
            android:layout_width="fill_parent"
            android:layout_height="fill_parent"></TableRow>

        <TableRow
            android:layout_width="fill_parent"
            android:layout_height="fill_parent"></TableRow>

        <TableRow
            android:layout_width="fill_parent"
